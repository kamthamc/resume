{
  "packages": [
    "packages/*"
  ],
  "version": "0.6.9",
  "command": {
    "create": {
      "homepage": "https://github.com/kamthamc/resume",
      "license": "MIT"
    },
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): publish %s [ci skip]",
      "forcePublish": true,
      "noGitReset": true,
      "signGitCommit": false,
      "signGitTag": false,
      "createRelease": "github"
    },
    "bootstrap": {
      "registry": "https://npm.pkg.github.com/kamthamc"
    }
  },
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md"
  ]
}
